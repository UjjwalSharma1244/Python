def find(arr,n):
    arr.sort()
    for i in range(n-1):
        l = i+1
        r = n-1
        x = arr[i]
        while(l<r):
            if arr[l] + arr[r] + x == 0:
                print(arr[l],arr[r],x)
                l += 1
                r -= 1
            elif arr[l] + arr[r] + x < 0:
                l += 1
            else:
                r -=1
    return -1

arr = [0,-1,2,-3,1]
n =len(arr)
find(arr,n)
