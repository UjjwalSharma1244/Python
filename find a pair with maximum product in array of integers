# Method 1 , Time complexity is O(n**2)

def product(arr,n):
    a = arr[0]
    b = arr[1]
    for i in range(0,n):
        for j in range(i+1,n):
            if arr[i] * arr[j] > a * b :
                a = arr[i]
                b = arr[j]

    print("maximum product is : {0} & {1}".format(a,b))

arr = [1,4,3,6,7,0]
n = len(arr)
product(arr,n)
